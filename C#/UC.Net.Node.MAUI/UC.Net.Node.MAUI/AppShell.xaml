<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="UC.Net.Node.MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:UC.Net.Node.MAUI.Pages"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="Dashboard"
        ContentTemplate="{DataTemplate pages:DashboardPage}"
        Route="DashboardPage" />
    
    <ShellContent
        Title="Account Details"
        ContentTemplate="{DataTemplate pages:AccountDetailsPage}"
        Route="AccountDetailsPage" />

</Shell>
