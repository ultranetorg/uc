<?xml version="1.0" encoding="UTF-8"?>
<StackLayout 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="UC.Umc.Controls.NextCancelButton"
    x:Name="Instance">
    <Button Command="{Binding NextCommand, Source={x:Reference Instance}}" Style="{StaticResource NextButton}" />
    <Button Command="{Binding CancelCommand, Source={x:Reference Instance}}" Style="{StaticResource LabelButton}" Text="Cancel" />
</StackLayout>