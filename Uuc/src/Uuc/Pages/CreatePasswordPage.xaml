<ContentPage x:Class="Uuc.Pages.CreatePasswordPage"
             xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <ScrollView>
        <VerticalStackLayout>
            <Label
                Text="CreatePasswordPage"
                Style="{StaticResource Headline}"
                SemanticProperties.HeadingLevel="Level1" />
            <Entry Text="{Binding Password}" />
            <Button Text="Create Password" Command="{Binding Submit_OnClickedCommand}" />
        </VerticalStackLayout>
    </ScrollView>

</ContentPage>
