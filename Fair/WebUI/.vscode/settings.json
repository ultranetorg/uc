{
  "cSpell.words": [
    "avenir",
    "brower",
    "grayscale",
    "lightgray",
    "singlefile",
    "szhsin",
    "uccs",
    "ultranet",
    "updation",
    "usehooks",
    "vite",
    "webui",
    "youtu"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "never"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "tailwindCSS.experimental.classRegex": [
    "className\\s*=\\s*[\"'`]([^\"'`]*)[\"'`]",
    "itemClassName\\s*=\\s*[\"'`]([^\"'`]*)[\"'`]",
    "activeItemClassName\\s*=\\s*[\"'`]([^\"'`]*)[\"'`]",
    "[A-Z0-9_]+_CLASSNAME\\s*=\\s*[\"'`]([^\"'`]*)[\"'`]"
  ],
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
}
